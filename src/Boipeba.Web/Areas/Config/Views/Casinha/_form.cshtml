<div class="modal fade" id="casinhaModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Casinha</h4>
            </div>
            <div class="modal-body">
                <form name="casinhaForm" ng-validate="{}">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="Nome">Nome</label>
                                <input type="text" class="form-control required" width="100" id="Nome" name="Nome" ng-model="viewdata.casinha.Nome" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="Descricao">Descrição</label>
                                <input type="text" class="form-control" width="100" id="Descricao" name="Descricao" ng-model="viewdata.casinha.Rua" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="Numero">Número</label>
                                <input type="text" class="form-control" width="100" id="Numero" name="Numero" ng-model="viewdata.casinha.Numero" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="DataCasinha">Data da Casinha</label>
                                <div class="input-group date" data-provide="datepicker" data-date-language="pt-BR">
                                    <input type="text" class="form-control" id="DataCasinha" name="DataCasinha" ng-model="viewdata.casinha.Data"/>
                                    <div class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                <button type="button" class="btn btn-primary" ng-click="validaForm(casinhaForm) && salvar(viewdata.casinha)">Salvar</button>
            </div>
        </div>
    </div>
</div>